[
  {
    "name": "Chrome DevTools MCP",
    "transport": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "chrome-devtools-mcp@latest"
    ],
    "enabled": true,
    "env": {}
  },
  {
    "name": "example-server",
    "transport": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "mcp-server-example"
    ],
    "enabled": true,
    "env": null
  },
  {
    "name": "context7",
    "transport": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp@latest"
    ],
    "enabled": true,
    "env": {
      "DEFAULT_MINIMUM_TOKENS": "10000"
    }
  }
]